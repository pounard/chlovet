# Composer managed packages
vendor
web/sites/all/modules/composer
web/sites/all/libraries/composer
web/sites/all/themes/composer

# Those files are copied or generated by Symfony bundles
web/bundles

# even after activating full project versioning in production you will still need
# to ignore theses files:
web/sites/default
web/sites/all/README.txt
#web/sites/default/default.settings.php
web/sites/example.sites.php
web/sites/sites.php
web/sites/README.txt

# In case you manage non salted /data directory in a /sites directory
# with symlinks from web/sites/default to /sites/default
# We do not want to store in git most of this content. Except the common.settings.php
# But do not forget this common.settings.php is not the one used by salt
# the real one should be in: .salt/files/common.settings.php
/web/sites/*/files
# /web/sites/*/modules/*
/web/sites/*/*settings*
# /web/sites/*/libraries/*
# /web/sites/*/modules/*
# /web/sites/*/themes/*
!/sites/*/common.settings.php
!/sites/*/local.settings.*.php

# Exceptions inside Drupal files (custom code).
# !web/sites/*/modules/custom
# !web/sites/*/themes/custom
# !web/sites/*/libraries/custom
# !web/monitor*

# WARNING: You will want to use your custom profile's real name below.
!web/profiles/chlovet

# npm modules
node_modules

# ignore runtime files
/lib/
/log
/private
/tmp
/var

# ignore apache conf
/etc/*

# Ignore IDE and OS-related files
.buildpath
.metadata
.settings
.project
.idea
.DS_Store
*.DS_Store
backup
nbproject
.hg
atlassian-ide-plugin.xml
*~
\#*#
*.mo
