{
    "name" : "chlovet/chlovet",
    "description" : "Chlovet Drupal site",
    "type" : "drupal-distribution",
    "license" : "Proprietary",
    "authors" : [
        {
            "name" : "Pierre Rineau",
            "email" : "pierre.rineau@makina-corpus.com"
        },
        {
            "name" : "Makina Corpus",
            "homepage" : "http://makina-corpus.com"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/apubsub.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-apubsub.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-badm.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-filechunk.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-gulpifier.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-iresponsive.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-minidialog.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-sf-dic.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-ucms.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-udate.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-ulog.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-unoderef.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-usync.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/redis-bundle.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:tfd7/tfd7.git"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require-dev" : {
        "phpunit/phpunit" : "^5.1"
    },
    "require" : {
        "composer/installers" : "^1.0.20",
        "cweagans/composer-patches" : "~1.0",
        "drush/drush": "7.*",
        "makinacorpus/apubsub" : "dev-master as 4.0",
        "makinacorpus/drupal-apubsub" : "dev-master as 4.0",
        "makinacorpus/drupal-badm" : "dev-master",
        "makinacorpus/drupal-filechunk" : "dev-master",
        "makinacorpus/drupal-gulpifier" : "dev-7.x-1.x",
        "makinacorpus/drupal-iresponsive" : "dev-master",
        "makinacorpus/drupal-minidialog" : "dev-master",
        "makinacorpus/drupal-sf-dic" : "dev-master as 3.0",
        "makinacorpus/drupal-ucms" : "dev-master",
        "makinacorpus/drupal-udate" : "dev-master",
        "makinacorpus/drupal-ulog" : "*",
        "makinacorpus/drupal-unoderef" : "dev-master as 1.0",
        "makinacorpus/drupal-usync" : "dev-master",
        "makinacorpus/redis-bundle" : "dev-master",
        "monolog/monolog": "^1.17",
        "drupal/drupal-extension": "~3.0",
        "symfony/dependency-injection" : "~3.0",
        "symfony/templating" : "~3.0",
        "symfony/twig-bridge" : "~3.0",
        "symfony/twig-bundle" : "~3.0",
        "tfd7/tfd7": "dev-master",
        "twig/extensions": "~1.3",
        "twig/twig": "~1.20|~2.0"
    },
    "config" : {
        "vendor-dir" : "lib/vendor"
    },
    "extra" : {
        "installer-paths": {
            "www" : [ "type:drupal-core" ],
            "www/sites/all/modules/composer/{$name}" : [ "type:drupal-module" ],
            "www/sites/all/libraries/composer/{$name}" : [ "type:drupal-library" ],
            "www/sites/all/themes/composer/{$name}" : [ "type:drupal-theme" ],
            "profiles/{$name}" : [ "type:drupal-profile" ]
        }
    },
    "autoload": {
        "psr-4": {
            "Chlovet\\Profile\\" : "www/profiles/chlovet/src"
        }
    }
}
