{
    "name" : "chlovet/chlovet",
    "description" : "Chlovet Drupal site",
    "type" : "drupal-distribution",
    "license" : "Proprietary",
    "authors" : [
        {
            "name" : "Pierre Rineau",
            "email" : "pierre.rineau@makina-corpus.com"
        },
        {
            "name" : "Makina Corpus",
            "homepage" : "http://makina-corpus.com"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-tooling.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/drupal-ucms.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/redis-bundle.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:tfd7/tfd7.git"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require" : {
        "composer/installers" : "^1.0.20",
        "cweagans/composer-patches" : "~1.0",
        "drush/drush": "7.*",
        "makinacorpus/apubsub" : "4.0.0",
        "makinacorpus/drupal-apubsub" : "4.0.0",
        "makinacorpus/drupal-badm" : "1.0.*",
        "makinacorpus/drupal-filechunk" : "1.0.*",
        "makinacorpus/drupal-gulpifier" : "1.0.*",
        "makinacorpus/drupal-iresponsive" : "1.0.*",
        "makinacorpus/drupal-latlonfield" : "1.0.*",
        "makinacorpus/drupal-minidialog" : "*",
        "makinacorpus/drupal-schedulefield" : "1.0.*",
        "makinacorpus/drupal-sf-dic" : "3.0.*",
        "makinacorpus/drupal-tooling" : "dev-master",
        "makinacorpus/drupal-plusql" : "1.0.*",
        "makinacorpus/drupal-ucms" : "dev-tree-flexible",
        "makinacorpus/drupal-udate" : "1.0.*",
        "makinacorpus/drupal-ulink" : "1.0.*",
        "makinacorpus/drupal-ulog" : "*",
        "makinacorpus/drupal-umenu" : "1.1.*",
        "makinacorpus/drupal-unoderef" : "1.0.*",
        "makinacorpus/drupal-usync" : "1.0.*",
        "makinacorpus/redis-bundle" : "dev-master",
        "monolog/monolog": "^1.17",
        "pear/console_table": "dev-master as 1.2.0",
        "symfony/dependency-injection" : "~3.0",
        "symfony/templating" : "~3.0",
        "symfony/twig-bridge" : "~3.0",
        "symfony/twig-bundle" : "~3.0",
        "tfd7/tfd7": "dev-master",
        "twig/extensions": "~1.3",
        "twig/twig": "~1.20|~2.0",
        "jbroadway/urlify": "^1.0",
        "oyejorge/less.php": "v1.7.0.10"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^5.1",
        "symfony/phpunit-bridge" : "^2.7"
    },
    "config" : {
        "bin-dir" : "bin",
        "vendor-dir" : "vendor"
    },
    "extra" : {
        "installer-paths": {
            "web" : [ "type:drupal-core" ],
            "web/sites/all/modules/composer/{$name}" : [ "type:drupal-module" ],
            "web/sites/all/libraries/composer/{$name}" : [ "type:drupal-library" ],
            "web/sites/all/themes/composer/{$name}" : [ "type:drupal-theme" ],
            "profiles/{$name}" : [ "type:drupal-profile" ]
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/",
            "Chlovet\\Profile\\" : "web/profiles/chlovet/src",
            "MakinaCorpus\\Ucms\\ContentList\\" : "web/profiles/chlovet/modules/ucms_extras/ucms_list/src",
            "MakinaCorpus\\Ucms\\Widget\\" : "web/profiles/chlovet/modules/ucms_extras/ucms_widget/src"
        }
    }
}
